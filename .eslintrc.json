{
	"extends": "wikimedia",
	"parserOptions": {
		"ecmaVersion": 6
	},
	"env": {
		"browser": true,
		"es6": true,
		"jquery": true,
		"mocha": true,
		"node": true,
		"qunit": true
	},
	"globals": {
		"browser": false,
		"require": false,
		"module": false,
		"mediaWiki": false,
		"mwPerformance": false,
		"OO": false
	},
	"rules": {
		"dot-notation": 0
	}
}
